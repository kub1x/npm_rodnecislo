{
  "name": "rodnecislo",
  "version": "1.4.0",
  "description": "A package for handling Czech and Slovak Personal ID",
  "main": "lib/rodnecislo.js",
  "types": "lib/rodnecislo.d.ts",
  "files": [
    "lib/"
  ],
  "keywords": [
    "personal",
    "ID",
    "Czech",
    "node",
    "module"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kub1x/rodnecislo"
  },
  "scripts": {
    "audit": "$npm_execpath audit",
    "watch": "babel ./src --out-dir ./ --copy-files --watch",
    "build": "babel ./src --out-dir ./ --copy-files",
    "check": "$npm_execpath outdated --depth 0",
    "coverage": "nyc --reporter=lcov --reporter=text --reporter=html $npm_execpath test",
    "coveralls": "$npm_execpath run coverage && cat ./coverage/lcov.info | coveralls && rm -rf ./coverage",
    "github-release": "conventional-github-releaser -p angular",
    "lint": "eslint src test",
    "test": "ava",
    "test:watch": "ava --watch",
    "typescript-test": "tsc --project test",
    "validate": "$npm_execpath run lint && $npm_execpath test",
    "prepare": "$npm_execpath run build && $npm_execpath run validate && $npm_execpath run check",
    "pp": "$npm_execpath run prepare",
    "major": "pmm major",
    "minor": "pmm minor",
    "patch": "pmm patch"
  },
  "author": {
    "name": "Jakub kub1x Podlaha",
    "email": "j.podlaha@gmail.com",
    "url": "http://kub1x.org"
  },
  "pre-commit": [
    "validate"
  ],
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/register": "^7.4.4",
    "ava": "^2.0.0",
    "babel-eslint": "^10.0.1",
    "conventional-github-releaser": "^3.1.3",
    "coveralls": "^3.0.4",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^6.0.0",
    "nyc": "^14.1.1",
    "pmm": "^2.0.0",
    "pre-commit": "^1.2.2",
    "sinon": "^7.3.2",
    "typescript": "^3.5.1"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "ava": {
    "files": [
      "test/*.js"
    ],
    "sources": [
      "src/**/*.js"
    ],
    "require": "@babel/register"
  }
}
